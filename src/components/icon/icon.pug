mixin Icon({id, modifier=""})
	-const classComponent = `${prefix}-icon`
	-const classTitleModifier = modifierFn(classComponent, modifier)
	-const classComponentJs = `${prefixjs}-icon`
	-const classTitleModifierJs = modifierFn(classComponentJs, modifier)
	svg(class=`${classComponent}${classTitleModifier} ${classComponentJs}${classTitleModifierJs}` viewbox="0 0 36 36" width=36 height=36)
		use(xlink:href=`#${id}`)
